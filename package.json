{
  "name": "xl-replit",
  "version": "0.1.0",
  "description": "Extra features for the Replit website.",
  "keywords": [],
  "author": "LuisAFK",
  "scripts": {
    "_old_watch": "webpack --config webpack/webpack.dev.js --watch",
    "_old_build": "webpack --config webpack/webpack.prod.js",
    "dev": "bash ./scripts/build.sh dev",
    "build": "bash ./scripts/build.sh prod",
    "dev:chrome": "bash ./scripts/build.sh dev chrome",
    "build:chrome": "bash ./scripts/build.sh prod chrome",
    "dev:firefox": "bash ./scripts/build.sh dev firefox",
    "build:firefox": "bash ./scripts/build.sh prod firefox",
    "clean": "rm -rf dist",
    "format": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lafkpages/XL-Replit.git"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.237",
    "@types/requirejs": "^2.1.34",
    "esbuild": "^0.18.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@replit/protocol": "^0.3.12",
    "monaco-editor": "^0.39.0",
    "requirejs": "^2.3.6"
  },
  "peerDependencies": {
    "zx": "^7.2.2"
  }
}
