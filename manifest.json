{
  "manifest_version": 3,
  "name": "XL Replit",
  "description": "Extra features for the Replit website.",
  "version": "1.1",
  "author": "LuisAFK",
  "icons": {
    "192": "favicon.png",
    "128": "favicon128.png"
  },
  "permissions": [
    
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.replit.com/*",
        "https://*.firewalledreplit.com/*"
      ],
      "css": ["content.css"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "inject.js" ],
      "matches": [ "https://*.replit.com/*", "https://*.firewalledreplit.com/*" ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "XL Replit",
    "default_icon": {
      "192": "favicon.png",
      "128": "favicon128.png"
    }
  }
}